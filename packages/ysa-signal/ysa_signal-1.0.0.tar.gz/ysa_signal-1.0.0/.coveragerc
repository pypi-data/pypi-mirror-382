[run]
source = .
omit =
    */tests/*
    */test_*.py
    setup.py
    setup_wizard.py
    extensions/*
    .venv/*
    venv/*
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract
