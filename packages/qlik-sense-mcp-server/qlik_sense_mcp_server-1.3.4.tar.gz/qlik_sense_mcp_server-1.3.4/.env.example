# Qlik Sense Server Configuration
# Required: Qlik Sense server URL
QLIK_SERVER_URL=https://your-qlik-server.company.com

# Required: User directory for authentication
QLIK_USER_DIRECTORY=COMPANY

# Required: User ID for authentication
QLIK_USER_ID=your-username

# Certificate Configuration (Required for production)
# Absolute path to client certificate file (.pem format)
QLIK_CLIENT_CERT_PATH=/absolute/path/to/certs/client.pem

# Absolute path to client private key file (.pem format)
QLIK_CLIENT_KEY_PATH=/absolute/path/to/certs/client_key.pem

# Absolute path to CA certificate file (.pem format)
# If not specified, SSL certificate verification will be disabled
QLIK_CA_CERT_PATH=/absolute/path/to/certs/root.pem

# Network Configuration
# Repository API port (default: 4242)
QLIK_REPOSITORY_PORT=4242

# Proxy API port for authentication (default: 4243)
QLIK_PROXY_PORT=4243

# Engine API port for WebSocket connections (default: 4747)
QLIK_ENGINE_PORT=4747

# Optional HTTP API port for metadata requests (no default)
# Only used for /api/v1/apps/{id}/data/metadata endpoint
# QLIK_HTTP_PORT=443

# SSL and Security
# Verify SSL certificates (true/false, default: true)
QLIK_VERIFY_SSL=false

# Timeouts and Performance
# HTTP request timeout in seconds (default: 10.0)
QLIK_HTTP_TIMEOUT=10.0

# WebSocket connection timeout in seconds (default: 8.0)
QLIK_WS_TIMEOUT=8.0

# Number of WebSocket connection retry attempts (default: 2)
QLIK_WS_RETRIES=2

# Logging
# Logging level (DEBUG, INFO, WARNING, ERROR, default: INFO)
LOG_LEVEL=INFO
