    },
    {
      "title": "",
      "description": "",
      "prompt": "I need to plan an event with Tim for 2hrs next week. Suggest some time slots."
    }
  ],
  "name": "mcp-gsuite",
  "repository": {
    "type": "git",
    "url": "https://github.com/MarkusPfundstein/mcp-gsuite"
  },
  "homepage": "https://github.com/MarkusPfundstein/mcp-gsuite",
  "author": {
    "name": "MarkusPfundstein"
  },
