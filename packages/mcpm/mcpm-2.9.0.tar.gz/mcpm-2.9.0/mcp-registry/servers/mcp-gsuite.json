{
  "display_name": "MCP Google Workspace Integration",
  "license": "MIT",
  "tags": [
    "gmail",
    "google",
    "calendar",
    "gsuite",
    "google workspace",
    "email",
    "productivity"
  ],
  "installations": {
    "uvx": {
      "type": "uvx",
      "command": "uvx",
      "args": [
        "mcp-gsuite",
        "--accounts-file",
        "${ACCOUNTS_FILE_PATH}",
        "--credentials-dir",
        "${CREDENTIALS_DIR}"
      ],
      "description": "Run the published mcp-gsuite server with custom account and credentials paths"
    }
  },
  "arguments": {
    "ACCOUNTS_FILE_PATH": {
      "description": "Path to the accounts file.",
      "required": true,
      "example": "/path/to/custom/.accounts.json"
    },
    "CREDENTIALS_DIR": {
      "description": "Path to the credentials directory.",
      "required": true,
      "example": "/path/to/custom/credentials"
    }
  },
  "examples": [
