<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xmi="http://www.omg.org/spec/XMI/20100901" targetNamespace="http://ratio-case.nl/ragraph" xmlns:rc="http://ratio-case.nl/ragraph">
    <xsd:import namespace="http://www.omg.org/spec/XMI/20100901" schemaLocation="XMI-Canonical.xsd" />
    <xsd:complexType name="Weight">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute name="value" type="xsd:float" />
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:element name="Weight" type="rc:Weight" />

    <xsd:complexType name="Annotation">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string" />
            <xsd:element name="type">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="boolean" />
                        <xsd:enumeration value="integer" />
                        <xsd:enumeration value="float" />
                        <xsd:enumeration value="string" />
                        <xsd:enumeration value="none" />
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="value" type="xmi:Any" />
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="Annotation" type="rc:Annotation" />

    <xsd:complexType name="Node">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="uuid" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="kind" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="label" type="xsd:string" minOccurs="0" />
            <xsd:element ref="rc:Weight" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element ref="rc:Annotation" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element name="parent" type="xsd:IDREF" minOccurs="0" maxOccurs="1" />
            <xsd:element name="child" type="xsd:IDREF" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element name="is_bus" type="xsd:boolean" minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
        <xsd:attribute ref="xmi:id" />
        <xsd:attributeGroup ref="xmi:ObjectAttribs" />
    </xsd:complexType>
    <xsd:element name="Node" type="rc:Node" />

    <xsd:complexType name="Edge">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="uuid" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="kind" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="label" type="xsd:string" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element ref="rc:Weight" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element ref="rc:Annotation" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element name="source" type="xsd:IDREF" minOccurs="1" maxOccurs="1" />
            <xsd:element name="target" type="xsd:IDREF" minOccurs="1" maxOccurs="1" />
        </xsd:sequence>
        <xsd:attribute ref="xmi:id" />
        <xsd:attributeGroup ref="xmi:ObjectAttribs" />
    </xsd:complexType>
    <xsd:element name="Edge" type="rc:Edge" />

    <xsd:complexType name="Graph">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="uuid" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="kind" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="label" type="xsd:string" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element ref="rc:Weight" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element ref="rc:Annotation" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element ref="rc:Node" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element ref="rc:Edge" minOccurs="0" maxOccurs="unbounded" />
        </xsd:sequence>
        <xsd:attribute ref="xmi:id" />
        <xsd:attributeGroup ref="xmi:ObjectAttribs" />
    </xsd:complexType>
    <xsd:element name="Graph" type="rc:Graph" />


</xsd:schema>