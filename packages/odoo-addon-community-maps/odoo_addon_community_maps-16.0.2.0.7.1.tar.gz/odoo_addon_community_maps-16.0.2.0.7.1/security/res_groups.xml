<odoo>
  <record id="community_maps_category" model="ir.module.category">
    <field name="name">Community maps access groups</field>
    <field name="description">
      Group access level to community maps models
    </field>
    <field name="sequence">10</field>
  </record>

  <record model="res.groups" id="group_community_maps_admin">
    <field name="name">Community maps administrator</field>
    <field name="category_id" ref="community_maps_category" />
  </record>
  <record model="res.groups" id="group_community_maps_manager">
    <field name="name">Community maps manager</field>
    <field name="category_id" ref="community_maps_category" />
  </record>
</odoo>
