# WAHA SDK Configuration
# Copy this file to .env and fill in your values

# WAHA server URL
WAHA_URL=http://localhost:3000

# API key for authentication (if required)
WAHA_API_KEY=your_api_key_here

# Test chat ID for sending messages
# Format: phone_number@c.us for individual chats
# Format: group_id@g.us for group chats
TEST_CHAT_ID=1234567890@c.us