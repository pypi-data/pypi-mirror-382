/**
 * {{ operation.summary or 'API operation' }}
 *
 * @method {{ operation.http_method }}
 * @path {{ operation.path }}
 */
export function {{ hook_name }}(
{%- if func_params -%}
{{ func_params }}
{%- endif -%}
) {
  return useSWR<{{ response_type }}>(
    {{ swr_key }},
{%- if fetcher_params %}
    () => Fetchers.{{ fetcher_name }}({{ fetcher_params }})
{%- else %}
    () => Fetchers.{{ fetcher_name }}()
{%- endif %}
  )
}

