{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "sqs:ReceiveMessage",
                "sqs:GetQueueAttributes",
                "sqs:GetQueueUrl",
                "sqs:DeleteMessage"
            ],
            "Resource": "arn:aws:sqs:REGION:ACCOUNT_NO:QUEUE_NAME"
        }
    ]
}
