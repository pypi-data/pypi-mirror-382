include README.md
include LICENSE
include requirements.txt

graft goodgleif/data
graft goodgleif/config

# Include only the manifest file - parquet files will be downloaded from GitHub
include goodgleif/data/*.txt

# Exclude all parquet files - they will be downloaded from GitHub releases
exclude goodgleif/data/*.parquet

global-exclude *.py[cod] __pycache__ .DS_Store


