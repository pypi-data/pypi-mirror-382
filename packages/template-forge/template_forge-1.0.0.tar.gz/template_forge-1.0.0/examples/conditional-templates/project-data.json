{
  "application": {
    "name": "AdvancedApp",
    "version": "2.0.0",
    "environment": "production",
    "debug_mode": false,
    "deployment_type": "docker",
    "features": [
      "authentication",
      "api",
      "websocket",
      "caching",
      "monitoring"
    ]
  },
  "database": {
    "type": "postgresql",
    "host": "localhost",
    "port": 5432,
    "name": "advancedapp_db",
    "pool_size": 20,
    "ssl": true
  },
  "api": {
    "version": "v2",
    "rate_limiting": {
      "enabled": true,
      "requests_per_minute": 100
    },
    "authentication": {
      "type": "JWT",
      "token_expiry_hours": 24
    },
    "endpoints": [
      {
        "path": "/users",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "auth_required": true
      },
      {
        "path": "/public/status",
        "methods": ["GET"],
        "auth_required": false
      },
      {
        "path": "/admin",
        "methods": ["GET", "POST"],
        "auth_required": true,
        "admin_only": true
      }
    ]
  },
  "caching": {
    "enabled": true,
    "backend": "redis",
    "ttl_seconds": 3600,
    "redis": {
      "host": "localhost",
      "port": 6379,
      "db": 0
    }
  },
  "monitoring": {
    "metrics_enabled": true,
    "tracing_enabled": true,
    "metrics": [
      "request_count",
      "response_time",
      "error_rate",
      "db_query_time",
      "cache_hit_rate"
    ]
  }
}
