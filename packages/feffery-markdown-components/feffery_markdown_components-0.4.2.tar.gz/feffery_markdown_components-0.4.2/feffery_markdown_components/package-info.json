{
  "name": "feffery_markdown_components",
  "version": "0.4.2",
  "description": "Better markdown render component in Plotly Dash.",
  "repository": {
    "type": "git",
    "url": "https://github.com/CNFeffery/feffery-markdown-components.git"
  },
  "bugs": {
    "url": "https://github.com/CNFeffery/feffery-markdown-components/issues"
  },
  "homepage": "https://github.com/CNFeffery/feffery-markdown-components",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve --config ./webpack.serve.config.js --open",
    "build:js": "webpack --mode production",
    "build:backends": "dash-generate-components ./src/lib/components feffery_markdown_components --keep-prop-order ALL -p package-info.json --r-prefix '' --jl-prefix '' --ignore \\.test\\.",
    "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py_and_r)",
    "build": "npm run build:js && npm run build:backends",
    "build:activated": "npm run build:js && npm run build:backends-activated"
  },
  "author": "CNFeffery <fefferypzy@gmail.com>",
  "email": "fefferypzy@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "antd": "^5.17.2",
    "deepdash-es": "^5.3.9",
    "less": "^3.13.1",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.21",
    "ramda": "^0.26.1",
    "react-copy-to-clipboard": "^5.0.4",
    "react-markdown": "^8.0.0",
    "react-syntax-highlighter": "^15.4.4",
    "react-use": "^17.4.0",
    "rehype-highlight": "^5.0.0",
    "rehype-katex": "^6.0.1",
    "rehype-raw": "^6.1.0",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.0",
