{
  "name": "AddressDictionary",
  "package": "com.boozallen.aiops.mda.pattern.dictionary",
  "dictionaryTypes": [
    {
      "name": "street",
      "simpleType": "string"
    },
    {
      "name": "city",
      "simpleType": "string"
    },
    {
      "name": "zipcode",
      "simpleType": "string",
      "validation": {
        "maxLength": 10,
        "minLength": 5,
        "formats": [
          "^\\d{5}(?:[-\\s]\\d{4})?$"
        ]
      }
    },
    {
      "name": "stateAddress",
      "simpleType": "string",
      "validation": {
        "maxLength": 2,
        "minLength": 2
      }
    }
  ]
}
