{
  "name": "City",
  "package": "com.boozallen.aiops.mda.pattern.record",
  "description": "Example City record for testing Spark Java Data Relations",
  "frameworks": [{
    "name": "pyspark"
  }],
  "relations": [
    {
      "name": "Mayor",
      "package": "com.boozallen.aiops.mda.pattern.records",
      "multiplicity": "1-1",
      "column": "MAYOR",
      "documentation": "There is one mayor in the city",
      "required": true
    },
    {
      "name": "State",
      "package": "com.boozallen.aiops.mda.pattern.records",
      "multiplicity": "M-1",
      "column": "STATE"
    },
    {
      "name": "Street",
      "package": "com.boozallen.aiops.mda.pattern.records",
      "multiplicity": "1-M",
      "column": "STREET"
    },
    {
      "name": "Citizen",
      "package": "com.boozallen.aiops.mda.pattern.records",
      "multiplicity": "1-M",
      "column": "CITIZEN",
      "required": true
    }
  ]
}
