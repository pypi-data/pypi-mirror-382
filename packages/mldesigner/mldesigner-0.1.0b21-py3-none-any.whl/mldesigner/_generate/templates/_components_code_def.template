{{node_name}} = {{component_func_name}}(
{% for name, val in params.items() %}
    {{name}}={{val}},
{% endfor %}
)
{% if sweep_objective %}
{{node_name}} = {{node_name}}.sweep({{sweep_objective}})
{% endif %}
{% if run_setting %}
{% for k, v in run_setting.items() %}
{% if v %}
{{node_name}}.{{k}} = {{v}}
{% else %}
{{node_name}}.{{k}}
{% endif %}
{% endfor %}
{% endif %}